<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.projectina.ina.Home">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/glossary"
        android:id="@+id/imageButtonGlossary"
        android:onClick="dispatchGlossaryIntent"
        android:layout_marginBottom="99dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/imageButtonMap" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/question"
        android:id="@+id/imageButtonQuestion"
        android:onClick="dispatchBackgroundIntent"
        android:layout_marginStart="55dp"
        android:layout_marginBottom="28dp"
        android:layout_alignBottom="@+id/imageButtonGlossary"
        android:layout_alignStart="@+id/imageButtonMap" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/test1"
        android:id="@+id/imageButtonMap"
        android:onClick="dispatchMapIntent"
        android:layout_marginTop="88dp"
        android:layout_marginStart="16dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/baby"
        android:id="@+id/imageButtonTrimesters"
        android:onClick="dispatchTrimesterIntent"
        android:layout_marginEnd="14dp"
        android:layout_alignBottom="@+id/imageButtonMap"
        android:layout_alignEnd="@+id/imageButtonGlossary"
        android:layout_marginBottom="25dp" />


</RelativeLayout>
